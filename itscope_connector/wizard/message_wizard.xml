<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="feedback_wizard_form" model="ir.ui.view">
            <field name="name">message.wizard.form</field>
            <field name="model">feedback.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <p>
                            <field name="message" readonly="1"/>
                        </p>
                        <footer>
                            <button name="action_ok" string="Ok" type="object" default_focus="1" class="oe_highlight"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>